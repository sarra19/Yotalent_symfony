<form method="post" action="{{ path('app_categorie_delete', {'idcat': categorie.idcat}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ categorie.idcat) }}">
 <button class="btn btn-secondary me-2"><i class="fas fa-trash"></i> Delete</button></form>
