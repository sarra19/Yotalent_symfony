{% extends 'Back.html.twig' %}

{% block body %}
    <h1>New comment</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.email) }}
    {{ form_row(form.comment) }}
    <button type="submit" class="btn btn-primary">Validate</button>
    {{ form_end(form) }}
{% endblock %}
